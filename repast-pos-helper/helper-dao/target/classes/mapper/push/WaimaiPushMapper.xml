<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuneke.mapper.push.WaimaiPushMapper">
  <resultMap id="BaseResultMap" type="com.yuneke.model.push.WaimaiPush">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="merchant_id" jdbcType="BIGINT" property="merchantId" />
    <result column="orderId" jdbcType="BIGINT" property="orderid" />
    <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
    <result column="type" jdbcType="TINYINT" property="type" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="data" jdbcType="VARCHAR" property="data" />
    <result column="count" jdbcType="TINYINT" property="count" />
  </resultMap>
  <sql id="Base_Column_List">
    id, merchant_id, orderId, create_date, type, status, data, count
  </sql>

  <update id="updateStatus">
    update t_waimai_push set status=1 where id&lt;=#{pushId} and merchant_id=#{merchantId} and orderId=#{orderId}
  </update>

</mapper>